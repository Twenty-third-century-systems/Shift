      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.js.map" />
      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.min.js" />
      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.min.js.map" />
    </ItemGroup>

    

    <ItemGroup>
      <Content Update="wwwroot\js\new-entity\application-reload.js">
        <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
        <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
      </Content>
      <Content Update="wwwroot\css\adminlte.css">
        <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
        <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
      </Content>
    </ItemGroup>

    

    <ItemGroup>
      <ProjectReference Include="..\Drinkers\Drinkers.csproj" />
    </ItemGroup>

