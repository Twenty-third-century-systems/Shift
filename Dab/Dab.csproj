    <ItemGroup>
      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.bundle.js" />
      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.bundle.js.map" />
      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.bundle.min.js" />
      <_ContentIncludedByDefault Remove="wwwroot\lib\jquery-smart-wizard\dist\js\bootstrap\bootstrap.bundle.min.js.map" />
